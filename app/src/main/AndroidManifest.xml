<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.quizup">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".FinalPackage.RootActivity">
        </activity>
        <activity android:name=".FinalPackage.ConnectActivity">
        </activity>
        <activity android:name=".Testing.Uitry">
        </activity>
        <activity android:name=".Testing.BotPlay">
        </activity>
        <activity android:name=".Testing.PvpPlay">
        </activity>
        <activity android:name=".Testing.RandomPlayBot">
        </activity>
        <activity android:name=".Testing.WhoToPlay">
        </activity>
        <activity android:name=".Homie.QuizMain">
        </activity>
        <activity android:name=".Homie.Categories">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>